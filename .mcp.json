{
  "mcpServers": {
    "portfolio-context": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@upstash/context7-mcp@latest"
      ],
      "env": {
        "CONTEXT7_PROJECT_SCOPE": "portfolio"
      }
    },
    "portfolio-memory": {
      "type": "stdio", 
      "command": "npx",
      "args": [
        "@movibe/memory-bank-mcp",
        "--mode", "code",
        "--project", "portfolio"
      ],
      "env": {}
    }
  }
}